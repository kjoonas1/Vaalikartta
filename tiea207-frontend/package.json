{
  "name": "tiea207-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "leaflet": "^1.5.1",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-leaflet": "^2.4.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "recharts": "^1.8.5",
    "shortid": "^2.2.15",
    "victory": "^33.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-pre-commit": "react-scripts test --env=jsdom --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint . --ext .js --ext .jsx",
    "lint-fix": "./node_modules/.bin/eslint . --ext .js --ext .jsx --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix && npm run test-pre-commit",
      "pre-push": "npm run lint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-test-renderer": "^16.9.0",
    "enzyme": "^3.10.0",
    "eslint": "^6.5.1",
    "eslint-plugin-prefer-arrow": "^1.1.6",
    "eslint-plugin-react": "^7.15.1",
    "husky": "^3.0.9",
    "jest": "^24.9",
    "jest-transform-stub": "^2.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  }
}
