{
    "name": "tiea207-frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@weknow/react-bubble-chart-d3": "^1.0.12",
        "axios": "^0.19.0",
        "leaflet": "^1.5.1",
        "node-sass": "^4.12.0",
        "react": "^16.9.0",
        "react-bootstrap": "^1.0.0-beta.12",
        "react-d3-library": "^1.1.8",
        "react-dom": "^16.9.0",
        "react-google-charts": "^3.0.15",
        "react-leaflet": "^2.4.0",
        "react-router-bootstrap": "^0.25.0",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.1.2",
        "shortid": "^2.2.15"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "test-pre-commit": "react-scripts test --env=jsdom --watchAll=false",
        "eject": "react-scripts eject",
        "lint": "./node_modules/.bin/eslint . --ext .js --ext .jsx",
        "lint-fix": "./node_modules/.bin/eslint . --ext .js --ext .jsx --fix"
    },
    "husky": {
        "hooks": {
            "post-merge": "npm i && npm run test-pre-commit",
            "pre-commit": "npm run lint-fix && npm run test-pre-commit",
            "pre-push": "npm run lint"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^4.2.0",
        "@testing-library/react": "^9.3.0",
        "babel-jest": "^24.9.0",
        "eslint": "^6.5.1",
        "eslint-plugin-prefer-arrow": "^1.1.6",
        "eslint-plugin-react": "^7.15.1",
        "husky": "^3.0.9",
        "jest": "^24.9.0",
        "jest-transform-stub": "^2.0.0",
        "react-test-renderer": "^16.9.0"
    },
    "jest": {
        "moduleNameMapper": {
            "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
        }
    }
}
